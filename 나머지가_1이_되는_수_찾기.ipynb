      },
      "outputs": [],
      "source": [
        "def solution(n):\n",
        "    answer = 0\n",
        "    for i in range(1,n):\n",
        "        if n%i==1:      #나머지가 1일때\n",
        "            answer=i    i를 답에 저장\n",
        "            break\n",
        "    return answer"
      ]
    }
  ]
}