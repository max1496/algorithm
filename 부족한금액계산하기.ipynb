      "source": [
        "def solution(price, money, count):\n",
        "    answer = -1\n",
        "    b=0\n",
        "    for i in range(1,count+1):\n",
        "        b+=price*i  #필요한돈\n",
        "    if b>money:\n",
        "        answer=b-money  #필요한돈 - 가진돈\n",
        "    else:\n",
        "        answer=0    #가진돈보다 적으면 0\n",
        "    return answer"
      ]
    }
  ]
