      },
      "outputs": [],
      "source": [
        "def solution(a, b):\n",
        "    answer = 0\n",
        "    if a<b:     #b가 a보다 크면 a부터\n",
        "        for i in range(a,b+1):\n",
        "            answer+=i\n",
        "    else:       #a가 b보다 크면 b부터\n",
        "        for i in range(b,a+1):\n",
        "            answer+=i\n",
        "    return answer"
      ]
    }
  ]
}